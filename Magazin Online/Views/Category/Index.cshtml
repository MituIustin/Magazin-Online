@{
    ViewData["Title"] = "Category Index";
}

<h2>Categories</h2>

<table class="table">
    <thead>
        <tr>
            <th>Category ID</th>
            <th>Category Name</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var category in ViewBag.Categories)
        {
            <tr>
                <td>@category.CategoryId</td>
                <td>@category.Name</td>
                
            </tr>
        }
    </tbody>
</table>
